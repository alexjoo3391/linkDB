<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" styleClass="container" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.AddLinkController">
	<children>
		<Label layoutX="30.0" layoutY="51.0" text="링크" />
		<TextField fx:id="AddLinkLink" layoutX="88.0" layoutY="47.0" prefHeight="23.0" prefWidth="291.0" promptText="http://www.example.com" />
		<Label layoutX="30.0" layoutY="87.0" text="분류" />
		<MenuButton fx:id="AddLinkClass" layoutX="88.0" layoutY="83.0" mnemonicParsing="false" text="분류">
			<items>
				<MenuItem fx:id="classAction1" mnemonicParsing="false" onAction="#SelectedClass" text="웹사이트" />
				<MenuItem fx:id="classAction2" mnemonicParsing="false" onAction="#SelectedClass" text="영상" />
            <MenuItem fx:id="classAction21" mnemonicParsing="false" onAction="#SelectedClass" text="음악" />
            <MenuItem fx:id="classAction211" mnemonicParsing="false" onAction="#SelectedClass" text="문서" />
			</items>
		</MenuButton>
		<Label layoutX="30.0" layoutY="126.0" text="세부분류" />
		<TextField fx:id="AddLinkDetailClass" layoutX="88.0" layoutY="122.0" prefHeight="23.0" prefWidth="135.0" />
		<Label layoutX="30.0" layoutY="162.0" text="설명" />
		<Button layoutX="324.0" layoutY="257.0" mnemonicParsing="false" onAction="#AddLink" text="추가" />
		<Button fx:id="closeBtn" layoutX="270.0" layoutY="257.0" mnemonicParsing="false" onAction="#Close" text="취소" />
		<TextField fx:id="AddLinkDesc" layoutX="88.0" layoutY="158.0" prefHeight="23.0" prefWidth="291.0" />
	</children>
</AnchorPane>
